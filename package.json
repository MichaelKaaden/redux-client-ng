{
  "name": "redux-client",
  "version": "1.0.5",
  "description": "A little redux example",
  "scripts": {
    "analyze": "webpack --config webpack.analyze.js",
    "build": "grunt && yarn run test && webpack --config webpack.prod.js",
    "build-fast": "webpack --config webpack.prod.js",
    "dev": "yarn && grunt && webpack-dev-server --config webpack.dev.js --open --progress --colors",
    "precommit": "yarn run tslint-fix",
    "prod": "yarn run build && node server.js",
    "prod-fast": "node server.js",
    "stats": "webpack --config webpack.prod.js --profile --json > webpack-stats.json",
    "test": "grunt && karma start karma.conf.js",
    "test-dev": "grunt && karma start --no-single-run karma.conf.js",
    "test-dev-chrome": "grunt && karma start --no-single-run test/karma.conf.js --browsers ChromeDebugging",
    "tslint": "tslint -c tslint.json \"app/scripts/{,**}/*.ts\" \"test/{,**}/*.ts\"",
    "tslint-fix": "tslint -c tslint.json --force --fix -t stylish \"app/scripts/{,**}/*.ts\" \"test/{,**}/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelKaaden/redux-client-ng.git"
  },
  "keywords": [
    "AngularJS",
    "Redux",
    "TypeScript",
    "Webpack"
  ],
  "author": "Michael Kaaden <github@kaaden.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelKaaden/redux-client-ng/issues"
  },
  "homepage": "https://github.com/MichaelKaaden/redux-client-ng#readme",
